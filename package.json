{
    "name": "vscode-markdown-link-suggestions",
    "displayName": "MarkDown Link Suggestions",
    "description": "Suggests local files and header anchors in MarkDown links",
    "version": "8.0.0",
    "license": "MIT",
    "categories": [
        "Programming Languages",
        "Linters"
    ],
    "keywords": [
        "markdown",
        "link",
        "intellisense",
        "autocomplete"
    ],
    "publisher": "TomasHubelbauer",
    "repository": {
        "type": "git",
        "url": "https://github.com/TomasHubelbauer/vscode-markdown-link-suggestions.git"
    },
    "engines": {
        "vscode": "^1.23.0"
    },
    "activationEvents": [
        "workspaceContains:**/*.md",
        "onLanguage:markdown"
    ],
    "main": "./out/extension",
    "contributes": {
        "configuration": [
            {
                "title": "markdown-link-suggestions",
                "properties": {
                    "markdown-link-suggestions.allowFullSuggestMode": {
                        "type": "boolean",
                        "default": false
                    },
                    "markdown-link-suggestions.allowSuggestionsForHeaders": {
                        "type": "boolean",
                        "default": true
                    }
                }
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^7.0.43",
        "tslint": "^5.8.0",
        "typescript": "^2.6.1",
        "vscode": "^1.1.6"
    },
    "dependencies": {
        "@types/fs-extra": "^5.0.2",
        "fs-extra": "^6.0.0",
        "markdown-dom": "^0.0.5"
    }
}
