{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "outDir": "out",
        "lib": [
            "es6",
            "es2016.array.include",
            "esnext.asynciterable",
            "es2017.string"
        ],
        "sourceMap": true,
        "rootDir": "src",
        "strict": true,
        "noUnusedLocals": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "noUnusedParameters": true,
    },
    "exclude": [
        "node_modules",
        ".vscode-test",
        "icon"
    ]
}
